<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="match_parent"
	android:layout_height="match_parent" android:background="@drawable/background2">
	<RelativeLayout android:layout_width="match_parent"
		android:layout_height="match_parent" android:id="@+id/relativeLayout1">
		<TextView android:textColor="#000000" android:layout_below="@+id/undergradBTN"
			android:layout_alignParentRight="true" android:id="@+id/textView2"
			android:layout_height="wrap_content" android:textAppearance="?android:attr/textAppearanceMedium"
			android:layout_marginRight="20dp" android:layout_width="wrap_content"
			android:text="News &amp; Events"></TextView>
		<TextView android:textColor="#000000" android:layout_below="@+id/undergradBTN"
			android:id="@+id/textView1" android:layout_alignParentLeft="true"
			android:layout_marginLeft="19dp" android:layout_height="wrap_content"
			android:textAppearance="?android:attr/textAppearanceMedium"
			android:layout_width="wrap_content" android:text="Undergraduate"></TextView>
		<ImageButton android:layout_marginTop="57dp"
			android:layout_alignLeft="@+id/textView1"
			android:layout_alignParentTop="true" android:layout_marginLeft="18dp"
			android:layout_height="wrap_content" android:layout_width="wrap_content"
			android:id="@+id/undergradBTN" android:src="@drawable/undergraduate"
			android:background="@null"></ImageButton>
		<ImageButton android:layout_alignLeft="@+id/textView2"
			android:layout_height="wrap_content" android:layout_width="wrap_content"
			android:layout_alignTop="@+id/undergradBTN" android:id="@+id/news"
			android:src="@drawable/news" android:background="@null"></ImageButton>
		<ImageButton android:layout_alignLeft="@+id/textView2"
			android:layout_height="wrap_content" android:layout_width="wrap_content"
			android:layout_alignTop="@+id/directory" android:id="@+id/tools"
			android:src="@drawable/tools" android:background="@null"></ImageButton>
		<TextView android:textColor="#000000" android:layout_below="@+id/directory"
			android:layout_alignRight="@+id/tools" android:id="@+id/textView4"
			android:layout_height="wrap_content" android:textAppearance="?android:attr/textAppearanceMedium"
			android:layout_marginRight="17dp" android:layout_width="wrap_content"
			android:text="Tools"></TextView>
		<TextView android:textColor="#000000"
			android:layout_alignRight="@+id/directory" android:id="@+id/textView3"
			android:layout_alignBottom="@+id/textView4"
			android:layout_alignBaseline="@+id/textView4" android:layout_height="wrap_content"
			android:textAppearance="?android:attr/textAppearanceMedium"
			android:layout_width="wrap_content" android:text="Directory"></TextView>
		<ImageButton android:layout_alignRight="@+id/undergradBTN"
			android:layout_height="wrap_content" android:layout_centerVertical="true"
			android:layout_width="wrap_content" android:id="@+id/directory"
			android:src="@drawable/directory" android:background="@null"></ImageButton>
		<ImageButton android:background="@null" android:layout_height="wrap_content"
			android:layout_width="wrap_content" android:id="@+id/imageButton1"
			android:src="@drawable/settings" android:layout_alignParentBottom="true"
			android:layout_alignParentLeft="true"></ImageButton>
	</RelativeLayout>

</LinearLayout>
